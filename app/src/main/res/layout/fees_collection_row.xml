<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/linearLayout_Normal_Vertical"
  android:layout_margin="@dimen/dp5"
    tools:ignore="RtlHardcoded,RtlSymmetry,SmallSp,ContentDescription,NestedWeights,HardcodedText,UselessParent,UseCompoundDrawables,DisableBaselineAlignment,UnusedAttribute,InefficientWeight">


    <android.support.v7.widget.CardView
        style="@style/cardViewStyle"
        android:layout_margin="1dp">


        <LinearLayout style="@style/linearLayout_Normal_Vertical">

            <LinearLayout style="@style/linearLayout_H_wrap">


                <TextView
                    style="@style/tv_weight_normal"
                    android:layout_margin="@dimen/dp5"
                    android:padding="@dimen/dp5"
                    android:text="@string/fees_collection_amp_expenses"
                    android:textSize="@dimen/sp12"
                    tools:ignore="InefficientWeight" />

                <ImageView
                    style="@style/tv_normal"
                    android:layout_margin="@dimen/dp5"
                    android:contentDescription="@string/image"
                    android:src="@drawable/ic_down_key_fill" />

                <ImageView
                    style="@style/imageView_NormalWH20"
                    android:layout_margin="@dimen/dp5"
                    android:contentDescription="@string/image"
                    android:src="@drawable/ic_sync_black_24dp" />

                <ImageView
                    style="@style/imageView_NormalWH20"
                    android:layout_margin="@dimen/dp5"
                    android:contentDescription="@string/image"
                    android:src="@drawable/ic_close_black_24dp" />


            </LinearLayout>

            <View
                style="@style/view_horizantal_H1WM"
                android:background="@color/backgroundColor2" />


            <LinearLayout
                style="@style/linearLayout_H_wrap"
                android:baselineAligned="false">

                <LinearLayout

                    style="@style/linearLayout_Normal_Vertical">

                    <LinearLayout
                        style="@style/Ll_both_match_heading">

                        <!--<com.github.mikephil.charting.charts.BarChart
                            android:id="@+id/fee_bar_chat"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp150"
                            android:layout_marginLeft="@dimen/dp10"
                            android:layout_marginRight="@dimen/dp10"
                            android:layout_marginBottom="@dimen/dp2"/>-->
                    </LinearLayout>

                    <LinearLayout
                        style="@style/linearLayout_Normal_Vertical"
                        android:layout_marginLeft="@dimen/dp15"
                        android:layout_marginRight="@dimen/dp15"
                        android:visibility="gone"
                        android:layout_marginBottom="@dimen/dp0">

                        <LinearLayout
                            android:gravity="center|left"
                            style="@style/linearLayout_H_wrap">

                            <TextView
                                android:layout_width="0dp"
                                android:layout_weight=".4"
                                android:layout_height="wrap_content"
                                android:drawableLeft="@drawable/ic_dot"
                                android:drawablePadding="@dimen/dp10"
                                android:drawableTint="@color/green"
                                android:gravity="center|left"
                                android:text="Collections :"
                                android:textColor="@color/black"
                                android:textSize="10sp" />

                            <TextView
                                android:id="@+id/tvCollection"
                                android:layout_width="0dp"
                                android:layout_weight=".6"
                                android:layout_height="wrap_content"
                                android:gravity="center|left"
                                android:paddingLeft="@dimen/dp15"
                                android:textSize="10sp"
                                android:textColor="@color/black"
                                android:text="10000" />
                        </LinearLayout>

                        <LinearLayout
                            style="@style/linearLayout_H_wrap"
                            android:gravity="center|left">

                            <TextView

                                android:layout_width="0dp"
                                android:layout_weight=".4"
                                android:layout_height="wrap_content"
                                android:drawableLeft="@drawable/ic_dot"
                                android:drawablePadding="@dimen/dp10"
                                android:drawableTint="@color/orange"
                                android:gravity="center|left"
                                android:textColor="@color/black"
                                android:text="Fees :"
                                android:textSize="10sp" />

                            <TextView
                                android:id="@+id/tvFees"
                                android:layout_width="0dp"
                                android:layout_weight=".6"
                                android:layout_height="wrap_content"
                                android:gravity="center|left"
                                android:paddingLeft="@dimen/dp15"
                                android:textSize="10sp"
                                android:textColor="@color/black"
                                android:text="4000" />

                        </LinearLayout>

                        <LinearLayout
                            style="@style/linearLayout_H_wrap"
                            android:gravity="center|left"
                           >

                            <TextView

                                android:layout_width="0dp"
                                android:layout_weight=".4"
                                android:layout_height="wrap_content"
                                android:drawableLeft="@drawable/ic_dot"
                                android:drawablePadding="@dimen/dp10"
                                android:drawableTint="@color/green"
                                android:gravity="center|left"
                                android:text="Express :"

                                android:textColor="@color/black"
                                android:textSize="10sp" />

                            <TextView
                                android:id="@+id/tvExpress"
                                android:layout_width="0dp"
                                android:layout_weight=".6"
                                android:layout_height="wrap_content"
                                android:gravity="center|left"
                                android:paddingLeft="@dimen/dp15"
                                android:textSize="10sp"
                                android:textColor="@color/black"
                                android:text="6000" />
                        </LinearLayout>
                    </LinearLayout>


                </LinearLayout>





            </LinearLayout>


        </LinearLayout>


    </android.support.v7.widget.CardView>

</LinearLayout>