<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp50"
    android:layout_margin="@dimen/dp0"
    android:background="@color/white"
    android:elevation="@dimen/dp1"
    android:orientation="horizontal"
    tools:ignore="RtlHardcoded,RtlSymmetry,SmallSp,ContentDescription,NestedWeights,HardcodedText,UselessParent,UseCompoundDrawables,DisableBaselineAlignment,UnusedAttribute,InefficientWeight">

    <LinearLayout
        style="@style/linearLayout_W0HM"
        android:layout_weight=".2"
        android:gravity="center"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="@dimen/dp60"
            android:layout_height="@dimen/dp45"
            android:layout_margin="@dimen/dp2"
            android:background="@drawable/image_backgroung"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:layout_width="@dimen/dp35"
                android:layout_height="@dimen/dp35"
                android:background="@drawable/ic_person_black_24dp" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        style="@style/linearLayout_W0HM"
        android:layout_weight=".75"
        android:gravity="center"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp40"
            android:layout_marginLeft="@dimen/dp5"
            android:layout_marginRight="@dimen/dp5"
            android:background="@drawable/card_shape"
            android:gravity="center"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/search"
                style="@style/imageViewNormal"
                android:layout_weight="0.2"
                android:contentDescription="@string/search"
                android:padding="@dimen/dp3"
                android:src="@drawable/ic_search_black_24dp" />

            <AutoCompleteTextView
                android:id="@+id/et_search"
                style="@style/Ll_weight_vtl"
                android:layout_height="@dimen/dp35"
                android:layout_weight="5"
                android:background="@null"
                android:hint="@string/search_member"
                android:padding="@dimen/dp5" />

            <ImageView
                android:id="@+id/voice"
                style="@style/imageViewNormal"
                android:layout_width="@dimen/dp0"
                android:layout_weight=".8"
                android:contentDescription="@string/voice_icon"
                android:padding="@dimen/dp5"
                android:src="@drawable/voice_icon" />

            <ImageView
                android:id="@+id/edit_setting"
                style="@style/imageViewNormal"
                android:layout_width="@dimen/dp0"
                android:layout_weight=".8"
                android:contentDescription="@string/settings"
                android:padding="@dimen/dp5"
                android:src="@drawable/ic_edit" />
            <ImageView
                android:id="@+id/barriers_setting"
                style="@style/imageViewNormal"
                android:layout_width="@dimen/dp0"
                android:layout_weight=".8"
                android:contentDescription="@string/settings"
                android:padding="@dimen/dp5"

                android:src="@drawable/ic_more_vert_black_24dp" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>